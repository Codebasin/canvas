#!/bin/sh -ex

ADMIN_PASS=turnkey
ADMIN_MAIL=admin@example.com
DOMAIN=www.example.com

SRC=/usr/local/src
WEBROOT=/var/www/canvas

# get db_pass and delete template rails app
DB_NAME=canvas
DB_USER=canvas
DB_PASS=$(grep password: $WEBROOT/config/database.yml | head -n 1 | awk '{print $2}')
rm -rf $WEBROOT

